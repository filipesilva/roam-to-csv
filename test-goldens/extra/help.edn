k/refs 45020 536870912] [45084 :block/string "[[Developer Notes]] " 536870912] [45084 :block/uid "S5UjGbjnq" 536870912] [45084 :create/time 1620211197682 536870912] [45084 :create/user 53 536870912] [45084 :edit/seen-by 3 536870912] [45084 :edit/seen-by 74 536872283] [45084 :edit/time 1620211198765 536870912] [45084 :edit/user 53 536870912] [45085 :block/uid "05-05-2021" 536870912] [45085 :create/time 1620223420360 536870912] [45085 :create/user 6897 536870912] [45085 :edit/time 1620223420360 536870912] [45085 :edit/user 6897 536870912] [45085 :log/id 1620223420359 536870912] [45085 :node/title "May 5th, 2021" 536870912] [45086 :block/uid "05-06-2021" 536870912] [45086 :create/time 1620269934561 536870912] [45086 :create/user 9445 536870912] [45086 :edit/time 1620269934561 536870912] [45086 :edit/user 9445 536870912] [45086 :log/id 1620269934560 536870912] [45086 :node/title "May 6th, 2021" 536870912] [45091 :block/heading 1 536870912] [45091 :block/open true 536870912] [45091 :block/order 1 536870912] [45091 :block/page 44917 536870912] [45091 :block/parents 44917 536870912] [45091 :block/string "[Security Advisories](https://github.com/Roam-Research/issues/security/advisories)" 536870912] [45091 :block/uid "H5L1gcS87" 536870912] [45091 :create/time 1620316831067 536870912] [45091 :create/user 3 536870912] [45091 :edit/time 1620316848581 536870912] [45091 :edit/user 3 536870912] [45093 :block/children 45094 536870912] [45093 :block/heading 1 536870912] [45093 :block/open true 536870912] [45093 :block/order 1 536870912] [45093 :block/page 44917 536870912] [45093 :block/parents 44917 536870912] [45093 :block/parents 45125 536870912] [45093 :block/string "[](https://github.com/Roam-Research/issues/security/policy#expectations)Expectations" 536870912] [45093 :block/uid "MpDTPrWG5" 536870912] [45093 :edit/seen-by 3 536870912] [45093 :edit/time 1620316867838 536870912] [45093 :edit/user 3 536870912] [45094 :block/children 45095 536870912] [45094 :block/children 45096 536870912] [45094 :block/children 45097 536870912] [45094 :block/open true 536870912] [45094 :block/order 0 536870912] [45094 :block/page 44917 536870912] [45094 :block/parents 44917 536870912] [45094 :block/parents 45093 536870912] [45094 :block/parents 45125 536870912] [45094 :block/string "When working with us according to this policy, you can expect us to:" 536870912] [45094 :block/uid "IJ3J22vNE" 536870912] [45094 :edit/seen-by 3 536870912] [45094 :edit/time 1620316867838 536870912] [45094 :edit/user 3 536870912] [45095 :block/open true 536870912] [45095 :block/order 0 536870912] [45095 :block/page 44917 536870912] [45095 :block/parents 44917 536870912] [45095 :block/parents 45093 536870912] [45095 :block/parents 45094 536870912] [45095 :block/parents 45125 536870912] [45095 :block/string "Work with you to understand and validate your report, including a timely initial response to the submission;" 536870912] [45095 :block/uid "-FakN4o8s" 536870912] [45095 :edit/seen-by 3 536870912] [45095 :edit/time 1620316867838 536870912] [45095 :edit/user 3 536870912] [45096 :block/open true 536870912] [45096 :block/order 1 536870912] [45096 :block/page 44917 536870912] [45096 :block/parents 44917 536870912] [45096 :block/parents 45093 536870912] [45096 :block/parents 45094 536870912] [45096 :block/parents 45125 536870912] [45096 :block/string "Work to remediate discovered vulnerabilities in a timely manner; and" 536870912] [45096 :block/uid "Y0U_rfiDV" 536870912] [45096 :edit/seen-by 3 536870912] [45096 :edit/time 1620316867838 536870912] [45096 :edit/user 3 536870912] [45097 :block/open true 536870912] [45097 :block/order 2 536870912] [45097 :block/page 44917 536870912] [45097 :block/parents 44917 536870912] [45097 :block/parents 45093 536870912] [45097 :block/parents 45094 536870912] [45097 :block/parents 45125 536870912] [45097 :block/string "Recognize your contribution to improving our security if you are the first to report a unique vulnerability, and your report triggers a code or configuration change." 536870912] [45097 :block/uid "XvvRF9EgK" 536870912] [45097 :edit/time 1620316867838 536870912] [45097 :edit/user 3 536870912] [45098 :block/children 45099 536870912] [45098 :block/children 45100 536870912] [45098 :block/heading 1 536870912] [45098 :block/open true 536870912] [45098 :block/order 2 536870912] [45098 :block/page 44917 536870912] [45098 :block/parents 44917 536870912] [45098 :block/parents 45125 536870912] [45098 :block/string "[](https://github.com/Roam-Research/issues/security/policy#official-communication-channels)Official Communication Channels" 536870912] [45098 :block/uid "PkR9D1Pyz" 536870912] [45098 :edit/seen-by 3 536870912] [45098 :edit/time 1620316867838 536870912] [45098 :edit/user 3 536870912] [45099 :block/open true 536870912] [45099 :block/order 0 536870912] [45099 :block/page 44917 536870912] [45099 :block/parents 44917 536870912] [45099 :block/parents 45098 536870912] [45099 :block/parents 45125 536870912] [45099 :block/string "The primary communication channel in via email to[security@roamresearch.com](mailto:security@roamresearch.com). Afterwards communication proceeds to a[Security Advisory](https://help.github.com/en/github/managing-security-vulnerabilities/about-github-security-advisories)on GitHub." 536870912] [45099 :block/uid "8m66F9xdw" 536870912] [45099 :edit/time 1620316867838 536870912] [45099 :edit/user 3 536870912] [45100 :block/open true 536870912] [45100 :block/order 1 536870912] [45100 :block/page 44917 536870912] [45100 :block/parents 44917 536870912] [45100 :block/parents 45098 536870912] [45100 :block/parents 45125 536870912] [45100 :block/string "Security Advisories are separate from public issues in that only you and the Roam team can see them, which allows us to work on fixing the problem without publicizing it further for exploitation." 536870912] [45100 :block/uid "buGYTPieM" 536870912] [45100 :edit/time 1620316867838 536870912] [45100 :edit/user 3 536870912] [45101 :block/children 45102 536870912] [45101 :block/children 45103 536870912] [45101 :block/children 45104 536870912] [45101 :block/children 45105 536870912] [45101 :block/heading 1 536870912] [45101 :block/open true 536870912] [45101 :block/order 3 536870912] [45101 :block/page 44917 536870912] [45101 :block/parents 44917 536870912] [45101 :block/parents 45125 536870912] [45101 :block/string "[](https://github.com/Roam-Research/issues/security/policy#disclosure-policy)Disclosure Policy" 536870912] [45101 :block/uid "Nk2ARqnoL" 536870912] [45101 :edit/seen-by 3 536870912] [45101 :edit/time 1620316867838 536870912] [45101 :edit/user 3 536870912] [45102 :block/open true 536870912] [45102 :block/order 0 536870912] [45102 :block/page 44917 536870912] [45102 :block/parents 44917 536870912] [45102 :block/parents 45101 536870912] [45102 :block/parents 45125 536870912] [45102 :block/string "We follow a[Coordinated Disclosure](https://cheatsheetseries.owasp.org/cheatsheets/Vulnerability_Disclosure_Cheat_Sheet.html#responsible-or-coordinated-disclosure)model, also known as Responsible Disclosure." 536870912] [45102 :block/uid "n53Ipz24c" 536870912] [45102 :edit/time 1620316867838 536870912] [45102 :edit/user 3 536870912] [45103 :block/open true 536870912] [45103 :block/order 1 536870912] [45103 :block/page 44917 536870912] [45103 :block/parents 44917 536870912] [45103 :block/parents 45101 536870912] [45103 :block/parents 45125 536870912] [45103 :block/string "Coordinated disclosure attempts to find a reasonable middle ground between Private and Full disclosure models. With coordinated disclosure, the initial report is made privately, but with the full details being published once a patch has been made available." 536870912] [45103 :block/uid "zft2qQPhL" 536870912] [45103 :edit/time 1620316867838 536870912] [45103 :edit/user 3 536870912] [45104 :block/open true 536870912] [45104 :block/order 2 536870912] [45104 :block/page 44917 536870912] [45104 :block/parents 44917 536870912] [45104 :block/parents 45101 536870912] [45104 :block/parents 45125 536870912] [45104 :block/string "Addressing vulnerabilities requires time and resources. While discussing the initial report we'll agree with you on a deadline for addressing it. If this deadline is not met, then you may adopt the full disclosure approach, and publish the full details. In our case the default deadline is 90 days." 536870912] [45104 :block/uid "dAn9qM1kh" 536870912] [45104 :edit/time 1620316867838 536870912] [45104 :edit/user 3 536870912] [45105 :block/open true 536870912] [45105 :block/order 3 536870912] [45105 :block/page 44917 536870912] [45105 :block/parents 44917 536870912] [45105 :block/parents 45101 536870912] [45105 :block/parents 45125 536870912] [45105 :block/string "Google's Project Zero adopts a similar approach, where the full details of the vulnerability are published after 90 days regardless of whether or not the organisation has published a patch." 536870912] [45105 :block/uid "jl056ykei" 536870912] [45105 :edit/time 1620316867838 536870912] [45105 :edit/user 3 536870912] [45106 :block/children 45107 536870912] [45106 :block/heading 1 536870912] [45106 :block/open true 536870912] [45106 :block/order 4 536870912] [45106 :block/page 44917 536870912] [45106 :block/parents 44917 536870912] [45106 :block/parents 45125 536870912] [45106 :block/string "[](https://github.com/Roam-Research/issues/security/policy#ground-rules)Ground Rules" 536870912] [45106 :block/uid "S1mjdNXJA" 536870912] [45106 :edit/seen-by 3 536870912] [45106 :edit/time 1620316867838 536870912] [45106 :edit/user 3 536870912] [45107 :block/children 45108 536870912] [45107 :block/children 45109 536870912] [45107 :block/children 45110 536870912] [45107 :block/children 45111 536870912] [45107 :block/children 45112 536870912] [45107 :block/children 45113 536870912] [45107 :block/children 45114 536870912] [45107 :block/children 45115 536870912] [45107 :block/children 45116 536870912] [45107 :block/open true 536870912] [45107 :block/order 0 536870912] [45107 :block/page 44917 536870912] [45107 :block/parents 44917 536870912] [45107 :block/parents 45106 536870912] [45107 :block/parents 45125 536870912] [45107 :block/string "To encourage vulnerability research and to avoid any confusion between legitimate research and malicious attack, we ask that you attempt, in good faith, to:" 536870912] [45107 :block/uid "G2XvgQd_7" 536870912] [45107 :edit/time 1620316867838 536870912] [45107 :edit/user 3 536870912] [45108 :block/open true 536870912] [45108 :block/order 0 536870912] [45108 :block/page 44917 536870912] [45108 :block/parents 44917 536870912] [45108 :block/parents 45106 536870912] [45108 :block/parents 45107 536870912] [45108 :block/parents 45125 536870912] [45108 :block/string "Play by the rules. This includes following this policy and any other relevant agreements;" 536870912] [45108 :block/uid "ojy-XZG79" 536870912] [45108 :edit/time 1620316867838 536870912] [45108 :edit/user 3 536870912] [45109 :block/open true 536870912] [45109 :block/order 1 536870912] [45109 :block/page 44917 536870912] [45109 :block/parents 44917 536870912] [45109 :block/parents 45106 536870912] [45109 :block/parents 45107 536870912] [45109 :block/parents 45125 536870912] [45109 :block/string "Report any vulnerability youve discovered promptly;" 536870912] [45109 :block/uid "sZLr1EXtv" 536870912] [45109 :edit/time 1620316867838 536870912] [45109 :edit/user 3 536870912] [45110 :block/open true 536870912] [45110 :block/order 2 536870912] [45110 :block/page 44917 536870912] [45110 :block/parents 44917 536870912] [45110 :block/parents 45106 536870912] [45110 :block/parents 45107 536870912] [45110 :block/parents 45125 536870912] [45110 :block/string "Avoid violating the privacy of others, disrupting our systems, destroying data, and/or harming user experience;" 536870912] [45110 :block/uid "d3ZC0jWqk" 536870912] [45110 :edit/time 1620316867838 536870912] [45110 :edit/user 3 536870912] [45111 :block/open true 536870912] [45111 :block/order 3 536870912] [45111 :block/page 44917 536870912] [45111 :block/parents 44917 536870912] [45111 :block/parents 45106 536870912] [45111 :block/parents 45107 536870912] [45111 :block/parents 45125 536870912] [45111 :block/string "Use only the Official Communication Channels to discuss vulnerability information with us;" 536870912] [45111 :block/uid "BSgkWohDv" 536870912] [45111 :edit/time 1620316867838 536870912] [45111 :edit/user 3 536870912] [45112 :block/open true 536870912] [45112 :block/order 4 536870912] [45112 :block/page 44917 536870912] [45112 :block/parents 44917 536870912] [45112 :block/parents 45106 536870912] [45112 :block/parents 45107 536870912] [45112 :block/parents 45125 536870912] [45112 :block/string "Handle the confidentiality of details of any discovered vulnerabilities according to our Disclosure Policy;" 536870912] [45112 :block/uid "cKlQ9TagN" 536870912] [45112 :edit/time 1620316867838 536870912] [45112 :edit/user 3 536870912] [45113 :block/open true 536870912] [45113 :block/order 5 536870912] [45113 :block/page 44917 536870912] [45113 :block/parents 44917 536870912] [45113 :block/parents 45106 536870912] [45113 :block/parents 45107 536870912] [45113 :block/parents 45125 536870912] [45113 :block/string "Perform testing only on in-scope systems, and respect systems and activities which are out-of-scope;" 536870912] [45113 :block/uid "C8IIsKUpM" 536870912] [45113 :edit/time 1620316867838 536870912] [45113 :edit/user 3 536870912] [45114 :block/open true 536870912] [45114 :block/order 6 536870912] [45114 :block/page 44917 536870912] [45114 :block/parents 44917 536870912] [45114 :block/parents 45106 536870912] [45114 :block/parents 45107 536870912] [45114 :block/parents 45125 536870912] [45114 :block/string "If a vulnerability provides unintended access to data: Limit the amount of data you access to the minimum required for effectively demonstrating a Proof of Concept; and cease testing and submit a report immediately if you encounter any user data during testing, such as Personally Identifiable Information (PII), Personal Healthcare Information (PHI), credit card data, or proprietary information;" 536870912] [45114 :block/uid "tq0qJReP5" 536870912] [45114 :edit/time 1620316867838 536870912] [45114 :edit/user 3 536870912] [45115 :block/open true 536870912] [45115 :block/order 7 536870912] [45115 :block/page 44917 536870912] [45115 :block/parents 44917 536870912] [45115 :block/parents 45106 536870912] [45115 :block/parents 45107 536870912] [45115 :block/parents 45125 536870912] [45115 :block/string "You should only interact with test accounts you own or with explicit permission from the account holder; and" 536870912] [45115 :block/uid "NmSsLhCS_" 536870912] [45115 :edit/time 1620316867838 536870912] [45115 :edit/user 3 536870912] [45116 :block/open true 536870912] [45116 :block/order 8 536870912] [45116 :block/page 44917 536870912] [45116 :block/parents 44917 536870912] [45116 :block/parents 45106 536870912] [45116 :block/parents 45107 536870912] [45116 :block/parents 45125 536870912] [45116 :block/string "Do not engage in extortion." 536870912] [45116 :block/uid "GG_evbAFM" 536870912] [45116 :edit/time 1620316867838 536870912] [45116 :edit/user 3 536870912] [45117 :block/children 45118 536870912] [45117 :block/children 45123 536870912] [45117 :block/children 45124 536870912] [45117 :block/heading 1 536870912] [45117 :block/open true 536870912] [45117 :block/order 5 536870912] [45117 :block/page 44917 536870912] [45117 :block/parents 44917 536870912] [45117 :block/parents 45125 536870912] [45117 :block/string "[](https://github.com/Roam-Research/issues/security/policy#safe-harbor)Safe Harbor" 536870912] [45117 :block/uid "Z9_JodpvW" 536870912] [45117 :edit/seen-by 3 536870912] [45117 :edit/time 1620316867838 536870912] [45117 :edit/user 3 536870912] [45118 :block/children 45119 536870912] [45118 :block/children 45120 536870912] [45118 :block/children 45121 536870912] [45118 :block/children 45122 536870912] [45118 :block/open true 536870912] [45118 :block/order 0 536870912] [45118 :block/page 44917 536870912] [45118 :block/parents 44917 536870912] [45118 :block/parents 45117 536870912] [45118 :block/parents 45125 536870912] [45118 :block/string "When conducting vulnerability research according to this policy, we consider this research conducted under this policy to be:" 536870912] [45118 :block/uid "i7300r8U2" 536870912] [45118 :edit/time 1620316867838 536870912] [45118 :edit/user 3 536870912] [45119 :block/open true 536870912] [45119 :block/order 0 536870912] [45119 :block/page 44917 536870912] [45119 :block/parents 44917 536870912] [45119 :block/parents 45117 536870912] [45119 :block/parents 45118 536870912] [45119 :block/parents 45125 536870912] [45119 :block/string "Authorized in view of any applicable anti-hacking laws, and we will not initiate or support legal action against you for accidental, good faith violations of this policy;" 536870912] [45119 :block/uid "9rl7TyFgf" 536870912] [45119 :edit/time 1620316867838 536870912] [45119 :edit/user 3 536870912] [45120 :block/open true 536870912] [45120 :block/order 1 536870912] [45120 :block/page 44917 536870912] [45120 :block/parents 44917 536870912] [45120 :block/parents 45117 536870912] [45120 :block/parents 45118 536870912] [45120 :block/parents 45125 536870912] [45120 :block/string "Authorized in view of relevant anti-circumvention laws, and we will not bring a claim against you for circumvention of technology controls;" 536870912] [45120 :block/uid "QQY6w7wKD" 536870912] [45120 :edit/time 1620316867838 536870912] [45120 :edit/user 3 536870912] [45121 :block/open true 536870912] [45121 :block/order 2 536870912] [45121 :block/page 44917 536870912] [45121 :block/parents 44917 536870912] [45121 :block/parents 45117 536870912] [45121 :block/parents 45118 536870912] [45121 :block/parents 45125 536870912] [45121 :block/string "Exempt from restrictions in our Acceptable Usage Policy that would interfere with conducting security research, and we waive those restrictions on a limited basis; and" 536870912] [45121 :block/uid "YtXRwByTF" 536870912] [45121 :edit/time 1620316867838 536870912] [45121 :edit/user 3 536870912] [45122 :block/open true 536870912] [45122 :block/order 3 536870912] [45122 :block/page 44917 536870912] [45122 :block/parents 44917 536870912] [45122 :block/parents 45117 536870912] [45122 :block/parents 45118 536870912] [45122 :block/parents 45125 536870912] [45122 :block/string "Lawful, helpful to the overall security of the Internet, and conducted in good faith." 536870912] [45122 :block/uid "S7suMk6US" 536870912] [45122 :edit/time 1620316867838 536870912] [45122 :edit/user 3 536870912] [45123 :block/open true 536870912] [45123 :block/order 1 536870912] [45123 :block/page 44917 536870912] [45123 :block/parents 44917 536870912] [45123 :block/parents 45117 536870912] [45123 :block/parents 45125 536870912] [45123 :block/string "You are expected, as always, to comply with all applicable laws. If legal action is initiated by a third party against you and you have complied with this policy, we will take steps to make it known that your actions were conducted in compliance with this policy." 536870912] [45123 :block/uid "BZpd0Zon4" 536870912] [45123 :edit/time 1620316867838 536870912] [45123 :edit/user 3 536870912] [45124 :block/open true 536870912] [45124 :block/order 2 536870912] [45124 :block/page 44917 536870912] [45124 :block/parents 44917 536870912] [45124 :block/parents 45117 536870912] [45124 :block/parents 45125 536870912] [45124 :block/string "If at any time you have concerns or are uncertain whether your security research is consistent with this policy, please submit a report through one of our Official Channels before going any further." 536870912] [45124 :block/uid "wfyG05fz6" 536870912] [45124 :edit/time 1620316867838 536870912] [45124 :edit/user 3 536870912] [45125 :block/children 45093 536870912] [45125 :block/children 45098 536870912] [451