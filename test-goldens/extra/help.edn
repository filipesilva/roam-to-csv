 [11635 :edit/seen-by 3422 536870912] [11635 :edit/seen-by 6897 536870912] [11635 :edit/time 1616558161871 536870912] [11635 :edit/user 3 536870912] [11637 :block/open true 536870912] [11637 :block/order 2 536870912] [11637 :block/page 412 536870912] [11637 :block/parents 412 536870912] [11637 :block/parents 575 536870912] [11637 :block/parents 11635 536870912] [11637 :block/string "Here's how the delete process looks like:" 536870912] [11637 :block/uid "TR4AEAWG-" 536870912] [11637 :edit/seen-by 6897 536870912] [11637 :edit/time 1615398069164 536870912] [11637 :edit/user 6897 536870912] [11638 :block/open true 536870912] [11638 :block/order 3 536870912] [11638 :block/page 412 536870912] [11638 :block/parents 412 536870912] [11638 :block/parents 575 536870912] [11638 :block/parents 11635 536870912] [11638 :block/string "![](https://downloads.intercomcdn.com/i/o/295732754/ff8f074bb3b727dee7b2aea4/2021-02-04+11.59.25.gif)" 536870912] [11638 :block/uid "tl92ySwg1" 536870912] [11638 :edit/seen-by 6897 536870912] [11638 :edit/time 1615398077308 536870912] [11638 :edit/user 6897 536870912] [11639 :block/open true 536870912] [11639 :block/order 1 536870912] [11639 :block/page 412 536870912] [11639 :block/parents 412 536870912] [11639 :block/parents 575 536870912] [11639 :block/parents 11635 536870912] [11639 :block/refs 6660 536870912] [11639 :block/string "Please note that **this is deleting all the data in your graph**. If you want to first [export]([[Export]]) the contents of your graph as a back up, you can also do that first by the three dot menu, and select 'Export All'." 536870912] [11639 :block/uid "N1RvTAMnR" 536870912] [11639 :edit/seen-by 6897 536870912] [11639 :edit/time 1615398060283 536870912] [11639 :edit/user 6897 536870912] [11640 :block/string "" 536870912] [11640 :block/uid "eFfXMaWFb" 536870912] [11640 :edit/time 1615397994791 536870912] [11640 :edit/user 6897 536870912] [11641 :block/open true 536870912] [11641 :block/order 0 536870912] [11641 :block/p