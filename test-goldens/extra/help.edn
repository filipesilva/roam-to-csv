ents 12444 536870912] [12446 :block/string "Here, notes for September 20th-22nd are missing." 536870912] [12446 :block/uid "e4oSYImOT" 536870912] [12446 :edit/time 1616395417618 536870912] [12446 :edit/user 3 536870912] [12447 :block/open true 536870912] [12447 :block/order 2 536870912] [12447 :block/page 412 536870912] [12447 :block/parents 412 536870912] [12447 :block/parents 575 536870912] [12447 :block/parents 12444 536870912] [12447 :block/string "![](https://s3.amazonaws.com/cdn.freshdesk.com/data/helpdesk/attachments/production/64002863684/original/8S-OTfnsy_Y8nlk_gN0FikwAZvp0jf_Rsg.png?1600971627)" 536870912] [12447 :block/uid "1KyWe5mi4" 536870912] [12447 :edit/time 1616395417618 536870912] [12447 :edit/user 3 536870912] [12448 :block/open true 536870912] [12448 :block/order 3 536870912] [12448 :block/page 412 536870912] [12448 :block/parents 412 536870912] [12448 :block/parents 575 536870912] [12448 :block/parents 12444 536870912] [12448 :block/string "Something to know about Daily Notes is that although they are automatically created each day, they**only save when you use them.**" 536870912] [12448 :block/uid "vVALNSNn1" 536870912] [12448 :edit/time 1616395417618 536870912] [12448 :edit/user 3 536870912] [12449 :block/open true 536870912] [12449 :block/order 4 536870912] [12449 :block/page 412 536870912] [12449 :block/parents 412 536870912] [12449 :block/parents 575 536870912] [12449 :block/parents 12444 536870912] [12449 :block/string "A Daily Note will save in one of two cases:" 536870912] [12449 :block/uid "RqEg81Fav" 536870912] [12449 :edit/time 1616395417618 536870912] [12449 :edit/user 3 536870912] [12450 :block/open true 536870912] [12450 :block/order 5 536870912] [12450 :block/page 412 536870912] [12450 :block/parents 412 536870912] [12450 :block/parents 575 536870912] [12450 :block/parents 12444 536870912] [12450 :block/string "1. You write on the page." 536870912] [12450 :block/uid "HXa9tWfqh" 536870912] [12450 :edit/time 1616395417618 536870912] [12450 :edit/user 3 536870912] [12451 :block/open true 536870912] [12451 :block/order 6 536870912] [12451 :block/page 412 536870912] [12451 :block/parents 412 536870912] [12451 :block/parents 575 536870912] [12451 :block/parents 12444 536870912] [12451 :block/string "2. You linked to the Daily Note from a different page." 536870912] [12451 :block/uid "KAnC83RCX" 536870912] [12451 :edit/time 1616395417618 536870912] [12451 :edit/user 3 536870912] [12452 :block/open true 536870912] [12452 :block/order 7 536870912] [12452 :block/page 412 536870912] [12452 :block/parents 412 536870912] [12452 :block/parents 575 536870912] [12452 :block/parents 12444 536870912] [12452 :block/string "You can see this from the All Notes view (from the left sidebar). In the picture below, pages for September 17th and 19th were created but have no content." 536870912] [12452 :block/uid "0SNd2ovqa" 536870912] [12452 :edit/time 1616395417618 536870912] [12452 :edit/user 3 536870912] [12453 :block/open true 536870912] [12453 :block/order 8 536870912] [12453 :block/page 412 536870912] [12453 :block/parents 412 536870912] [12453 :block/parents 575 536870912] [12453 :block/parents 12444 536870912] [12453 :block/string "![](https://s3.amazonaws.com/cdn.freshdesk.com/data/helpdesk/attachments/production/64002864033/original/KhotePm9AM16UV7slmUCsMyt5zb02rEKBA.png?1600972704)" 536870912] [12453 :block/uid "kYsIgLBCA" 536870912] [12453 :edit/time 1616395417618 536870912] [12453 :edit/user 3 536870912] [12454 :block/open true 536870912] [12454 :block/order 9 536870912] [12454 :block/page 412 536870912] [12454 :block/parents 412 536870912] [12454 :block/parents 575 536870912] [12454 :block/parents 12444 536870912] [12454 :block/string "Both dates have one mention, though. You can see from the Linked References of the page that the Daily Note of September 17th was linked from the previous day." 536870912] [12454 :block/uid "IqP9Bdy3T" 536870912] [12454 :edit/time 1616395417618 536870912] [12454 :edit/us