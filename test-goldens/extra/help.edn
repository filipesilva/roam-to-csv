/user 6897 536870912] [12050 :edit/seen-by 3 536870912] [12050 :edit/time 1616392441428 536870912] [12050 :edit/user 3 536870912] [12051 :block/string "" 536870912] [12051 :block/uid "LSjm3uON5" 536870912] [12051 :edit/time 1616392441479 536870912] [12051 :edit/user 3 536870912] [12069 :block/children 12071 536870912] [12069 :block/children 12072 536870912] [12069 :block/children 12073 536870912] [12069 :block/children 12074 536870912] [12069 :block/children 12075 536870912] [12069 :block/children 12076 536870912] [12069 :block/children 12077 536870912] [12069 :block/children 12078 536870912] [12069 :block/children 12081 536870912] [12069 :block/children 12082 536870912] [12069 :block/children 12083 536870912] [12069 :block/heading 3 536870912] [12069 :block/open false 536870912] [12069 :block/order 0 536870912] [12069 :block/page 412 536870912] [12069 :block/parents 412 536870912] [12069 :block/parents 575 536870912] [12069 :block/string "**How do I create a graph?**" 536870912] [12069 :block/uid "l3FRRQEt-" 536870912] [12069 :create/time 1616392457663 536870912] [12069 :create/user 3 536870912] [12069 :edit/seen-by 3 536870912] [12069 :edit/time 1616558059445 536870912] [12069 :edit/user 3 536870912] [12071 :block/open true 536870912] [12071 :block/order 1 536870912] [12071 :block/page 412 536870912] [12071 :block/parents 412 536870912] [12071 :block/parents 575 536870912] [12071 :block/parents 12069 536870912] [12071 :block/string "The first page you will see is the graph overview page. A graph is another name for a database. Inside a graph is where you will do all the work in Roam, writing, making blocks and pages. And to get there, you have to create your first graph." 536870912] [12071 :block/uid "PFyipT9wo" 536870912] [12071 :edit/seen-by 3 536870912] [12071 :edit/time 1616392479091 536870912] [12071 :edit/user 3 536870912] [12072 :block/open true 536870912] [12072 :block/order 2 536870912] [12072 :block/page 412 536870912] [12072 :block/parents 412 536870912